<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="X" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date [%thread] %-5level \(%logger{46}\): %message%n</pattern>
        </encoder>
    </appender>
    <logger name="org.apache.wicket" level="INFO"/>
    <root level="DEBUG">
        <appender-ref ref="X"/>
    </root>
</configuration>
