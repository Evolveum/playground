<objectModification
	xmlns='http://midpoint.evolveum.com/xml/ns/public/common/api-types-2'
	xmlns:c='http://midpoint.evolveum.com/xml/ns/public/common/common-2a'
	xmlns:t="http://prism.evolveum.com/xml/ns/public/types-2">
	<oid>c0c010c0-d34d-b33f-f00d-111111111111</oid>
	<!-- Anakin has turned to the dark side of the force -->
	<modification>
		<t:modificationType>replace</t:modificationType>
		<t:value>
			<c:fullName>Darth Vader</c:fullName>
		</t:value>
	</modification>
	<!-- And is now watching over the construction of Death Star -->
	<modification>
		<t:modificationType>replace</t:modificationType>
		<t:value>
			<c:locality>Death Star</c:locality>
		</t:value>
	</modification>
	<!-- Give him a number -->
	<modification>
		<t:modificationType>replace</t:modificationType>
		<t:value>
			<c:employeeNumber>1</c:employeeNumber>
		</t:value>
	</modification>
</objectModification>
